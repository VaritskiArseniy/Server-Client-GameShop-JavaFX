<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #FEC8D0;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllerClasses.AddQuantityProgram">
    <children>
        <Text layoutX="31.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Показать программы">
            <font>
                <Font name="Arial" size="51.0" />
            </font>
        </Text>
        <Button fx:id="backBtn" layoutX="12.0" layoutY="354.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="78.0" style="-fx-background-color: transparent; -fx-text-fill: black; -fx-border-width: 2px 2px 2px 2px; -fx-border-color: black;" text="Назад">
            <font>
                <Font name="Calisto MT" size="13.0" />
            </font>
        </Button>
        <TableView fx:id="ProgramTableView" layoutX="50.0" layoutY="93.0" onMouseClicked="#addQuantity" prefHeight="200.0" prefWidth="598.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="genreColumn" prefWidth="75.0" text="Категория" />
                <TableColumn fx:id="priceColumn" prefWidth="75.0" text="Цена" />
                <TableColumn fx:id="soldColumn" prefWidth="75.0" text="Продано" />
                <TableColumn fx:id="quantityColumn" prefWidth="75.0" text="Количество" />
                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Название" />
                <TableColumn fx:id="descriptionColumn" prefWidth="75.0" text="Описание" />
                <TableColumn fx:id="OSColumn" prefWidth="75.0" text="Операционная система" />
            </columns>
        </TableView>
      <TextField fx:id="quantityField" layoutX="285.0" layoutY="325.0" promptText="Сколько товаров" />
    </children>
</AnchorPane>
